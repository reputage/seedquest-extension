{
  "name": "SeedQuest",
  "version": "0.1",
  "description": "Build an Extension!",
  "permissions": ["activeTab", "declarativeContent", "storage"],
  "manifest_version": 2,

  "icons": {
    "16": "images/seedquest16.png",
    "32": "images/seedquest32.png",
    "48": "images/seedquest48.png",
    "128": "images/seedquest128.png"
  },
  
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
	"browser_action": {
      "default_popup": "popup.html",
      "default_title": "SeedQuest",
      "default_icon": {
        "16": "images/seedquest16.png",
        "32": "images/seedquest32.png",
        "48": "images/seedquest48.png",
        "128": "images/seedquest128.png"
      }
  }

}
